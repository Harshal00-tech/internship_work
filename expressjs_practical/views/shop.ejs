<%- include('includes/header.ejs') %>
<main>
    <center>
        <h3>My Products</h3>
    </center>
    <div id="products-container">
        <% if(products.length > 0) { %>
            <% for(product of products) { %>
                <center>
                    <div class="card">
                        <img class="card-image" src="data:image/jpeg;base64," alt="Card Image">
                        <h2 class="card-title"><%= product.title %></h2>
                        <p class="card-description">This is a brief description of the card content. It gives more context to the image and title.</p>
                    </div>
                    <!-- Repeat above card for each product -->
                </center>
            <% } %>
        <% } else {  %>
        
            <!-- Else show message -->
            <!-- If no products available -->
            <center>
                <p>Please select items.</p>
            </center>
        <% } %>
    </div>
</main>
</body>
</html>
